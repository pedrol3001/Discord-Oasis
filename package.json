{
  "name": "discord-oasis",
  "version": "1.0.5",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm2015/index.js",
  "typings": "./dist/typings/index.d.ts",
  "files": [
    "/dist"
  ],
  "private": false,
  "author": " <pedrolcr@outlook.com>",
  "license": "MIT",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:es6 && yarn build:es2022 && yarn build:types",
    "build:cjs": "tsc",
    "build:es6": "tsc -p ./typescript/tsconfig.esm6.json",
    "build:es2022": "tsc -p ./typescript/tsconfig.esm2022.json",
    "build:types": "tsc -p ./typescript/tsconfig.types.json",
    "clean": "rimraf ./dist",
    "publish:dev": "npm publish --registry http://localhost:4873/",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "@types/uuid": "^8.3.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "discord.js": "^13.3.1",
    "prisma": "^3.6.0",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "description": "Discordjs bot framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrol3001/Discord-Oasis.git"
  },
  "keywords": [
    "discord",
    "bot",
    "framework",
    "commands",
    "plugins",
    "modular"
  ],
  "bugs": {
    "url": "https://github.com/pedrol3001/Discord-Oasis/issues"
  },
  "homepage": "https://github.com/pedrol3001/Discord-Oasis#readme"
}
